<div class="card">
    <img class="card__image" src={{ project.data.imageTablet }} alt={{ project.data.imageAlt }}>
    <p class="tag">{{ project.data.eyebrow }}</p>
    <div class="card__details">
        <h2><a href="{{ project.url }}">{{ project.data.cardTitle}}</a></h2>
        <p>{{ project.data.blurb }}</p>
    </div>
</div>